{
   "version": "1.0.0-*",
   "testRunner": "xunit",
   "dependencies": {
           "Microsoft.NETCore.App": {
      "version": "1.0.1",
      "type": "platform"
    },
      "<%= name %>": {
         "target": "project"
      },
      "dotnet-test-xunit": "2.2.0-preview2-build1029",
      "xunit": "2.2.0-beta2-build3300",
      "OpenCover": "4.6.519",
      "OpenCoverToCoberturaConverter": "0.2.4",
      "ReportGenerator": "2.4.5"
   },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },
   "buildOptions": {
      "copyToOutput": {
         "include": [
            "xunit.runner.json"
         ]
      }
   },
   "tooling": {
      "defaultNamespace": "<%= name %>.Tests"
   }
}